rows_num = int(input())

final_rows = []

for r in range(0, rows_num):

    final_rows.append([int(row) for row in input().split()])

attacked_squares = input().split()
destroyed_ships = 0

for i in attacked_squares:
    row_col = i.split("-")
    row = int(row_col[0])
    col = int(row_col[1])

    if final_rows[row][col] > 0:
        final_rows[row][col] -= 1
        if final_rows[row][col] == 0:
            destroyed_ships += 1
    else:
        continue


print(destroyed_ships)


--------------------------------------------------------


first_player = False
second_player = False

first_player_count = 0
second_player_count = 0

total_squares = 0

for i in range(3):
    move = input().split()

    for a in move:
        if int(a) == 1:
            first_player_count += 1
            second_player_count = 0
            if first_player_count == 3:
                first_player = True
                print(f"First player won")
                break

        elif int(a) == 2:
            second_player_count += 1
            first_player_count = 0
            if second_player_count == 3:
                second_player = True
                print(f"Second player won")
                break
        else:
            pass
        total_squares += 1
    if first_player:
        break
    if second_player:
        break

if (not first_player and not second_player and total_squares == 9) or (first_player and second_player and total_squares == 9):
    print(f"Draw!")
    
    
---------------------------------------------------------------


numbers = input().split(" ")
jump = int(input())
iter = jump - 1
result = ""

while True:
    if len(numbers) == 0:
        break

    while iter >= len(numbers):
        iter = iter - len(numbers)

    result += f"{numbers[iter]},"
    numbers.pop(iter)

    iter += jump - 1

result = result[:-1]
print(f"[{result}]")


